module <%= object.path %>Docs
<% unless object.docstring.empty? %>
  def docs_class
  <<-DOCS
  * DOCS: ~#{object.path}~
  <%= object.docstring %>
  DOCS
  end
<% end %>
<% object.meths.each do |method| %>
  <% next if method.docstring.empty? %>
  def docs_<%= method.name %>
  <<-DOCS
  * DOCS: ~#{method.path}~
  <%= method.docstring %>
  DOCS
  end
<% end %>
end
<%= object.path %>.extend Docs
<%= object.path %>.extend <%= object.path %>Docs
